{
  "name": "express-pg-drizzle-ts-starter",
  "version": "1.0.0",
  "description": "A starter repo for scaffolding a express server with TS, PG, Drizzle ORM",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "db:generate-migration": "drizzle-kit generate",
    "db:drop-migration": "drizzle-kit drop",
    "db:migrate": "ts-node ./src/drizzle/migrate.ts",
    "db:studio": "drizzle-kit studio --verbose"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.22.8",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.4",
    "express": "^4.21.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "winston": "^3.15.0",
    "zod": "^3.23.8"
  }
}
